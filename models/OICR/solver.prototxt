train_net: "models/OICR/train.prototxt"
base_lr: 0.001
lr_policy: "warm_step"
gamma: 0.1
# stepsize: 40000
stepvalue: 10000
stepvalue: 40000
display: 20
average_loss: 100
iter_size: 2
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "vgg16_oicr"
#debug_info: true
